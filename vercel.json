{
  "version": 2,
  "installCommand": "echo 'Skipping installation'",
  "buildCommand": "mkdir dist && cp index.html index.tsx metadata.json dist/ && cp -r src dist/",
  "outputDirectory": "dist",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/.*",
      "dest": "/index.html"
    }
  ]
}